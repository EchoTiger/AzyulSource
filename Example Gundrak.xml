<HBProfile>
	<Name>Gundrak - Remote Version</Name>

	<Blacklist>
		<Mob Name="Drakkari Frenzy" Id="29834" Flags="Pull,Combat" />
		<Mob Name="Small Frog" Id="13321" Flags="Pull,Combat" /> 
	</Blacklist>

	<QuestOrder>
		<CustomBehavior File="RunCode" Code="" /> <!-- Do we have the behaviors? -->

		<CustomBehavior File="RunCode" Type="Definition" Code="
			string[] ProtectedItems = {'example', 'example 2'};
			string[] DepositItems = {'example', 'example 2'}; 
		" />

		<CustomBehavior File="LoadRemoteCode" />

		<CustomBehavior  File="RemoteCode" CodeUrl="http://enu.li/Snips/ProfileSettings.xml" /> <!-- Activate Profile Settings -->
		<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/UniversalNav/ToNorthrend.xml" /> <!-- Go to Northrend -->
		<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/Snips/DefensiveHooks.xml" /> <!-- Activate Defensive Manuvers -->

		<While Condition="true" >
			<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/DungeonEnter/Gundrak.xml" /> <!-- Enter Gundrak -->
			<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/DungeonRun/Gundrak.xml" /> <!-- Run Gundrak -->
			<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/Snips/GuildBank.xml" /> <!-- Guild Bank Stuff -->
			<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/DungeonExit/Gundrak.xml" /> <!-- Exit Gundrak -->
			<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/DungeonVendor/ItemsAndMounts.xml" /> <!-- Vendor by Mounts/Items/Etc -->
			<CustomBehavior File="RemoteCode" CodeUrl="http://enu.li/DungeonVendor/Gundrak.xml" /> <!-- Vendor by Gundrak Vendors -->
		</While>

	</QuestOrder>
</HBProfile>